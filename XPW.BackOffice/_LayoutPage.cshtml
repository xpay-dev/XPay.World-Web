<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!-- Favicon icon -->
    <link rel="icon" href="@Url.Content("~/Assets/images/favicon.ico")" type="image/x-icon">
    <!-- fontawesome icon -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/fonts/fontawesome/css/fontawesome-all.min.css")">
    <!-- animation css -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/plugins/animation/css/animate.min.css")">
    <!-- notification css -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/plugins/notification/css/notification.min.css")">
    <!-- vendor css -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/css/style.css")">

</head>
<body>
    <div>
        @RenderBody()
    </div>

    <!-- Required Js -->
    <script src="@Url.Content("~/Assets/js/vendor-all.min.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/bootstrap/js/bootstrap.min.js")"></script>

    <script src="assets/js/pcoded.min.js"></script>
    <!-- amchart js -->
    <script src="@Url.Content("~/Assets/plugins/amchart/js/amcharts.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/gauge.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/serial.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/light.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/pie.min.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/ammap.min.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/usaLow.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/radar.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/worldLow.js")"></script>
    <!-- notification Js -->
    <script src="@Url.Content("~/Assets/plugins/notification/js/bootstrap-growl.min.js")"></script>

    <!-- dashboard-custom js -->
    <script src="@Url.Content("~/Assets/js/pages/dashboard-custom.js")"></script>
</body>
</html>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!-- Favicon icon -->
    <link rel="icon" href="@Url.Content("~/Assets/images/favicon.ico")" type="image/x-icon">
    <!-- fontawesome icon -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/fonts/fontawesome/css/fontawesome-all.min.css")">
    <!-- animation css -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/plugins/animation/css/animate.min.css")">
    <!-- notification css -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/plugins/notification/css/notification.min.css")">
    <!-- vendor css -->
    <link rel="stylesheet" href="@Url.Content("~/Assets/css/style.css")">

</head>
<body>

    <!-- [ navigation menu ] start -->
    <nav class="pcoded-navbar">
        <div class="navbar-wrapper">
            <div class="navbar-brand header-logo">
                <a href="index.html" class="b-brand">
                    <div class="b-bg">
                        <i class="feather icon-trending-up"></i>
                    </div>
                    <span class="b-title">Xpay</span>
                </a>
                <a class="mobile-menu" id="mobile-collapse" href="#!"><span></span></a>
            </div>
            <div class="navbar-content scroll-div">
                <ul class="nav pcoded-inner-navbar">
                    <li class="nav-item pcoded-menu-caption">
                        <label>Navigation</label>
                    </li>
                    <li class="nav-item active pcoded-trigger">
                        <a href="#!" class="nav-link"><span class="pcoded-micon"><i class="feather icon-home"></i></span><span class="pcoded-mtext">Dashboard</span></a>
                    </li>

                    <li class="nav-item">
                        <a href="#!" class="nav-link"><span class="pcoded-micon"><i class="feather icon-home"></i></span><span class="pcoded-mtext">Merchant Management</span></a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- [ navigation menu ] end -->
    <!-- [ Header ] start -->
    <header class="navbar pcoded-header navbar-expand-lg navbar-light">
        <div class="m-header">
            <a class="mobile-menu" id="mobile-collapse1" href="#!"><span></span></a>
            <a href="index.html" class="b-brand">
                <div class="b-bg">
                    <i class="feather icon-trending-up"></i>
                </div>
                <span class="b-title">Index</span>
            </a>
        </div>
        <a class="mobile-menu" id="mobile-header" href="#!">
            <i class="feather icon-more-horizontal"></i>
        </a>
        <div class="collapse navbar-collapse">

            <ul class="navbar-nav ml-auto">
                <li>
                    <div class="dropdown drp-user">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="icon feather icon-settings"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right profile-notification">
                            <div class="pro-head">
                                <img src="assets/images/user/avatar-1.jpg" class="img-radius" alt="User-Profile-Image">
                                <span>Joel Doe</span>
                                <a href="auth-signin.html" class="dud-logout" title="Logout">
                                    <i class="feather icon-log-out"></i>
                                </a>
                            </div>
                           
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </header>
    <!-- [ Header ] end -->

    <div>
        @RenderBody()
    </div>

    <!-- Required Js -->
    <script src="@Url.Content("~/Assets/js/vendor-all.min.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/bootstrap/js/bootstrap.min.js")"></script>

    <script src="assets/js/pcoded.min.js"></script>
    <!-- amchart js -->
    <script src="@Url.Content("~/Assets/plugins/amchart/js/amcharts.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/gauge.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/serial.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/light.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/pie.min.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/ammap.min.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/usaLow.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/radar.js")"></script>
    <script src="@Url.Content("~/Assets/plugins/amchart/js/worldLow.js")"></script>
    <!-- notification Js -->
    <script src="@Url.Content("~/Assets/plugins/notification/js/bootstrap-growl.min.js")"></script>

    <!-- dashboard-custom js -->
    <script src="@Url.Content("~/Assets/js/pages/dashboard-custom.js")"></script>
</body>
</html>
